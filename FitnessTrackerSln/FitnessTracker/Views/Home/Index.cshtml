@model IQueryable<Workout>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Lift</th>
            <th>Reps</th>
            <th>Weight</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var w in Model) {
            <tr>
                <td>@w.WorkoutID</td>
                <td>@w.Lift</td>
                <td>@w.Reps</td>
                <td>@w.Weight</td>
                <td>@w.WorkoutDate</td>
                <td><a asp-action="Details" asp-route-id="@w.WorkoutID">Details</a></td>
                <td><a asp-action="Edit" asp-route-id="@w.WorkoutID">Edit</a></td>
                <td><a asp-action="Delete" asp-route-id="@w.WorkoutID">Delete</a></td>
            </tr>
        }
    </tbody>
</table>
<a asp-action="Create">Create</a>